<% if (!user) { %>
	<p>Welcome! Please <a href="/login">log in</a>.</p>	
<% } else { %>
	<p>Hello, <%= user.username %>. View your <a href="/profile">profile</a>. <a href="/logout">logout</a></p>
<% } %>
<div class="body-row">
	<div class="repositories-grid">
       <div class="row grid-container">
			<% for(var i=0; i < repos.length; i++) { %>
			<div class="col-xl-4 col-lg-4 col-md-6 col-sm-12 col-12 grid-item-container">
				<div class="grid-item-body">
					<div class="author-header clearfix">
						<a href="<%= repos[i].repo.owner.avatar_url %>" target="_blank">
							<div class="author-img">
								<img src="<%= repos[i].repo.owner.avatar_url %>" alt="facebookresearch">
							</div><div class="author-details">
								<h5><%= repos[i].repo.owner.username %></h5>
								<p class="small text-muted">View Profile</p>
							</div>
						</a>
						</div>
						<div class="repo-header">
							<h5>
								<a href="<%= repos[i].repo.owner.userProfileUrl %>" target="_blank">
									<span class="repo-name"><%= repos[i].repo.owner.username %></span></a>
								</h5>
								<p class="repo-meta text-muted small">Built by · <a target="_blank" href="<%= repos[i].repo.owner.userProfileUrl %>"><%= repos[i].repo.owner.username %></a> · October 24, 2018</p>
							</div>
							<div class="repo-body">
								<p><%= repos[i].repo.description %></p>
							</div>
							<div class="repo-footer">
								<span class="d-inline-block mr-3">
									<span class="repo-language-color ml-0"></span>
									<span itemprop="programmingLanguage"><%= repos[i].repo.language %></span>
								</span>
								<a class="muted-link d-inline-block mr-3" href="https://github.com/facebookresearch/maskrcnn-benchmark/stargazers" target="_blank">
									<svg viewBox="0 0 14 16" version="1.1" width="14" height="16" role="img">
										<path fill-rule="evenodd" d="M14 6l-4.9-.64L7 1 4.9 5.36 0 6l3.6 3.26L2.67 14 7 11.67 11.33 14l-.93-4.74L14 6z">

										</path></svg><%= repos[i].repo.stargazers_count %></a><a class="muted-link d-inline-block mr-3" href="https://github.com/facebookresearch/maskrcnn-benchmark/network/members" target="_blank">
											<svg viewBox="0 0 10 16" version="1.1" width="10" height="16" role="img"><path fill-rule="evenodd" d="M8 1a1.993 1.993 0 0 0-1 3.72V6L5 8 3 6V4.72A1.993 1.993 0 0 0 2 1a1.993 1.993 0 0 0-1 3.72V6.5l3 3v1.78A1.993 1.993 0 0 0 5 15a1.993 1.993 0 0 0 1-3.72V9.5l3-3V4.72A1.993 1.993 0 0 0 8 1zM2 4.2C1.34 4.2.8 3.65.8 3c0-.65.55-1.2 1.2-1.2.65 0 1.2.55 1.2 1.2 0 .65-.55 1.2-1.2 1.2zm3 10c-.66 0-1.2-.55-1.2-1.2 0-.65.55-1.2 1.2-1.2.65 0 1.2.55 1.2 1.2 0 .65-.55 1.2-1.2 1.2zm3-10c-.66 0-1.2-.55-1.2-1.2 0-.65.55-1.2 1.2-1.2.65 0 1.2.55 1.2 1.2 0 .65-.55 1.2-1.2 1.2z"></path></svg><%= repos[i].repo.forks_count %></a>
											<a class="muted-link d-inline-block mr-3" href="https://github.com/facebookresearch/maskrcnn-benchmark/watchers" target="_blank"><svg viewBox="0 0 16 16" version="1.1" width="16" height="16" aria-hidden="true"><path fill-rule="evenodd" d="M8.06 2C3 2 0 8 0 8s3 6 8.06 6C13 14 16 8 16 8s-3-6-7.94-6zM8 12c-2.2 0-4-1.78-4-4 0-2.2 1.8-4 4-4 2.22 0 4 1.8 4 4 0 2.22-1.78 4-4 4zm2-4c0 1.11-.89 2-2 2-1.11 0-2-.89-2-2 0-1.11.89-2 2-2 1.11 0 2 .89 2 2z"></path></svg><%= repos[i].repo.watchers_count %></a>
										</div>
									</div>
								</div>




								<% } %> -->
	   <!-- </div>
	</div>
</div>
